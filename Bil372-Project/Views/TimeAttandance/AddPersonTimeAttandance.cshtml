@model Bil372_Project.Models.Time_AttandanceModel
@using Bil372_Project.Models
@{
    Layout = "~/Views/Shared/AdminMain.cshtml";}
@{
    var data = (PersonelModel)ViewData["Message"];
}



<body>
    <div class="mid-class">
        <div class="art-right-w3ls">
            <h2> Add New Sertificate to Person</h2>
            @using (Html.BeginForm("getSelectedValue", "TimeAttandance", FormMethod.Post, new { enctype = "multipart/form-data", @class = "form-horizontal", role = "form" }))
            {
                <div class="form-left-to-w3l">
                    <div class="col-md-6">
                        <label>Person Name:</label>
                    </div>
                    @Html.DropDownList("Persons", ViewBag.Persons as SelectList)
                </div>
                <div class="form-left-to-w3l">
                    <label for="giris_saati">Giriş Saati:</label>
                    <input type="datetime-local" id="giris_saati" name="giris_saati">
                </div>
                <div class="form-left-to-w3l">
                    <label for="cikis_saati">Çıkış Saati:</label>
                    <input type="datetime-local" id="cikis_saati" name="cikis_saati">
                </div>


                <div class="btnn">
                    <button type="submit">Add</button>
                    <br />
                </div>

                <div class="clear"></div>
            }

        </div>

    </div>

</body>


